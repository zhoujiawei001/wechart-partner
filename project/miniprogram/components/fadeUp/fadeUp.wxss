/* components/fadeUp/fadeUp.wxss */
.box {
  position: absolute;
  left: 0;
  bottom: 152rpx;
  width: 100%;
}
.content {
  position: relative;
  background-color: #fff;
  border-radius: 8rpx;
  z-index: 10;
}
.box .item {
  height: 84rpx;
  line-height: 84rpx;
  border-bottom: 1rpx solid #ddd;
  z-index: 11;
  font-size: 28rpx;
  box-sizing: border-box;
  padding-left: 24rpx;
}
.mask {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0,.05);
  z-index: 9;
}
@keyframes slidedown {
    from {
        bottom: 152rpx;
    }
    to {
        bottom: 0;
    }
}
.slidedown {
    animation: slidedown 0.3s linear ;
}
.slideup {
    animation: slideup 0.3s linear ;
}
@keyframes slideup {
    from {
        bottom: 0;
    }
    to {
        bottom: 152rpx;
    }
}